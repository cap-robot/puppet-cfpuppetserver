{
  "name": "codingfuture-cfpuppetserver",
  "version": "0.9.5",
  "author": "Andrey Galkin",
  "summary": "Post-setup configuration to live in peace with cfnetwork and cfsystem modules",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cfpuppetserver",
  "project_page": "https://github.com/codingfuture/puppet-cfpuppetserver",
  "issues_url": "https://github.com/codingfuture/puppet-cfpuppetserver/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "15.10",
        "16.04",
        "16.10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.0.0 <5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.12.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.2.2"
    },
    {
      "name": "puppetlabs-postgresql",
      "version_requirement": ">= 4.7.1"
    },
    {
      "name": "puppetlabs-puppetdb",
      "version_requirement": ">= 5.1.2"
    },
    {
      "name": "codingfuture-cfnetwork",
      "version_requirement": ">= 0.9.8"
    },
    {
      "name": "codingfuture-cfauth",
      "version_requirement": ">= 0.9.6"
    }
  ],
  "data_provider": "hiera"
}
